{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": "airbnb-base",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"linebreak-style": 0,
		"global-require": 0,
		"eslint linebreak-style": [0, "error", "windows"],
		"indent": [2, "tab"],
		"no-tabs": 0,
		"no-console": "off",
		"quotes": ["error", "double", { "avoidEscape": true, "allowTemplateLiterals": true }]
	}
}
